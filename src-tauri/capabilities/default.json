{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "默认能力：主窗口可访问 dialog、core、sql、http 等",
  "windows": ["main", "*"],
  "permissions": [
    "core:default",
    "dialog:default",
    "sql:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://skills.sh" },
        { "url": "https://skills.sh/*" }
      ]
    }
  ]
}
